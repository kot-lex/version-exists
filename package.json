  },
  "author": "ntwcklng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntwcklng/version-exists/issues"
  },
  "homepage": "https://github.com/ntwcklng/version-exists#readme",
  "devDependencies": {
    "eslint-config-prettier": "1.6.0",
    "husky": "0.13.3",
    "lint-staged": "3.4.0",
    "prettier": "0.22.0",
    "xo": "0.18.1"
  },
  "dependencies": {
    "args": "^2.6.1",
    "chalk": "^1.1.3",
    "node-fetch": "^1.6.3",
    "npm-name": "^3.0.0"
  }
