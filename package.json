{
  "name": "version-exists",
  "version": "0.0.1",
  "description": "Check if version of a NPM module exists",
  "files": [
    "bin",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "version-exists": "bin/version-exists.js"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "npm run lint",
    "lint": "xo"
  },
  "repository": "ntwcklng/version-exists",
  "engines": {
    "node": ">= 6.6.0"
  },
  "keywords": [
    "cli",
    "command",
    "arguments",
    "util",
    "bin",
    "version",
    "check"
  ],
  "xo": {
